{
  "name": "co-browser-storage",
  "version": "0.5.1",
  "description": "Browser storage mangement for Angular 2 using ngrx/store",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "build": "npm run typescript && npm run sass",
    "watch": "tsc -p . -w",
    "typescript": "tsc -p .",
    "sass": "gulp sass",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calleboketoft/co-browser-storage.git"
  },
  "keywords": [
    "angular2"
  ],
  "author": "Carl-Ola Boketoft",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calleboketoft/co-browser-storage/issues"
  },
  "homepage": "https://github.com/calleboketoft/co-browser-storage#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@ngrx/store": "1.5.0",
    "es6-shim": "0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "bootstrap": "git+https://git@github.com/twbs/bootstrap.git#v4-dev",
    "express": "4.13.4",
    "ghooks": "1.0.3",
    "gulp": "3.9.1",
    "gulp-sass": "2.2.0",
    "typescript": "1.8.10",
    "typings": "0.8.1"
  }
}
