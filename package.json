{
  "name": "co-browser-storage",
  "version": "1.0.0",
  "description": "Browser storage mangement for Angular 2 using ngrx/store",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "build": "typings install && npm run typescript && npm run sass",
    "watch": "tsc -w",
    "typescript": "tsc",
    "sass": "gulp sass",
    "typings": "typings",
    "test-unit": "karma start",
    "test": "npm run test-unit"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calleboketoft/co-browser-storage.git"
  },
  "keywords": [
    "angular2"
  ],
  "author": "Carl-Ola Boketoft",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calleboketoft/co-browser-storage/issues"
  },
  "homepage": "https://github.com/calleboketoft/co-browser-storage#readme",
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.4",
    "@ngrx/store": "^2.0.1"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@ngrx/core": "1.0.1",
    "@ngrx/store": "2.0.1",
    "bootstrap": "git+https://git@github.com/twbs/bootstrap.git#v4-dev",
    "es6-shim": "0.35.0",
    "express": "4.13.4",
    "ghooks": "1.0.3",
    "gulp": "3.9.1",
    "gulp-sass": "2.2.0",
    "jasmine-core": "2.4.1",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-systemjs": "0.14.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "typescript": "1.8.10",
    "typings": "1.1.0",
    "zone.js": "0.6.12"
  }
